{
  "name": "usabilitydynamics/wp-network",
  "description": "WordPress Network",
  "type": "wordpress-plugin",
  "homepage": "https://github.com/UsabilityDynamics/wp-network",
  "keywords": [
    "wordpress",
    "multisite",
    "varnish"
  ],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Andy Potanin",
      "homepage": "http://usabilitydynamics.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
      "url": "http://repository.usabilitydynamics.com"
    },
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    }
  ],
  "config": {
    "preferred-install": "dist",
    "github-protocols": [ "ssh" ],
    "vendor-dir": "vendor",
    "bin-dir": "vendor/bin",
    "optimize-autoloader": true
  },
  "require": {
    "php": ">=5.3",
    "composer/installers": "v1.0.18",
    "usabilitydynamics/lib-settings": "0.2.2"
  },
  "autoload": {
    "classmap": [
      "lib/class-bootstrap.php",
      "lib/class-utility.php",
      "lib/class-list-table.php"
    ]
  }
}
